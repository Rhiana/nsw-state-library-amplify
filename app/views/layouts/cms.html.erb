<!DOCTYPE html>
<html>
  <head>
    <title>Admin CMS</title>
    <%= stylesheet_link_tag    'application', 'https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.css' %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= stylesheet_link_tag    'application', 'https://fonts.googleapis.com/css?family=Muli' %>
    <%= stylesheet_link_tag    'application', 'https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css' %>
    <%= stylesheet_link_tag    'application', 'https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.6/css/bootstrap.min.css' %>
    <%= stylesheet_link_tag    'application', '/assets/css/app.css?a=1499756908' %>
    <%= stylesheet_link_tag    'application', '/nsw-state-library-amplify/assets/css/styles.css?a=1499756908' %>
    <%= stylesheet_link_tag    'application', '/assets/css/app.css?a=1499756908' %>
    <%= stylesheet_link_tag    'search_auto_complete' %>

    <%= javascript_include_tag 'application', 'https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js'%>
    <%= javascript_include_tag 'application', 'https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.js'%>

    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'search_auto_complete' %>

    <%= csrf_meta_tags %>
  </head>
  <body>
    <div id="app" class="container-fluid">
      <div id="header" class="header">
        <nav id="primary-navigation" class="navigation primary inverse" role="navigation">
          <div>
            <div id="header-title" class="title-container">  <style type="text/css">    .header .title-container {      width: auto;      float: left;    }  </style>  <div class="header-bar">    <a href="/" title="Amplify" class="title" aria-label="Click to return to homepage">      <img src="/nsw-state-library-amplify/assets/img/logo_cropped.png" alt="Logo" class="logo">      <p class="logo-text">Amplify</p>    </a>  </div></div>
            <div id="header-crumbs" class="crumbs-container"></div>
            <div id="account-container" class="account-container">
              <div class="select active">
                <div class="select-active" title="">
                  <span class="score">0</span> CMS</div>
                  <div class="select-options account-menu menu" role="menu" aria-hidden="true" aria-label="User account menu">
                    <a href="/dashboard" class="select-option menu-item" role="menuitem">Dashboard</a>
                    <a href="/admin" class="select-option menu-item" role="menuitem">Admin</a>
                    <a href="/admin/cms" class="select-option menu-item" role="menuitem">CMS</a>
                    <a href="#sign-out" class="sign-out-link select-option menu-item" role="menuitem">Sign out</a>
                  </div>
                </div>
              </div>

            <div id="primary-menu-container" class="menu-container">
              <div class="header-menu menu" role="menu">
                <a href="/" class="menu-item " role="menuitem">Browse</a>
                <a href="/search" class="menu-item " role="menuitem">Search</a>
                <a href="/page/collections" class="menu-item " role="menuitem">Collections</a>
                <a href="/page/about" class="menu-item " role="menuitem">About</a>
                <a href="/page/faq" class="menu-item " role="menuitem">FAQ</a>
              </div>
            </div>
          </div>
        </nav>
        <nav id="secondary-navigation" class="navigation secondary"></nav>
        <div id="primary-alert" class="alert" role="alert" aria-atomic="true"></div>
      </div>

      <div id="main" class="body">
        <%= yield :cms_heading %>
        <%= yield %>
      </div>

      <div id="footer" class="footer">
        <nav id="footer-navigation" class="navigation inverse">
          <div id="footer-menu-container" class="menu-container"></div>
        </nav>
      </div>
    </div>

    <script src="/nsw-state-library-amplify/project.js"></script>
    <script src="/assets/js/templates.js"></script>
    <script src="/assets/js/admin.js"></script>
    <script src="/nsw-state-library-amplify/assets/js/custom.js"></script>
  </body>
</html>
